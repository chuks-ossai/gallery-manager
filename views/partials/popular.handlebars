<div class="card mt-3">
    <div class="card-header">
        <h5 class="panel-title">Most Popular</h5>
    </div>
    <div class="card-body">
        <div class="row text-center">
            {{#each sidebar.popular}}
            <div class="col-6 pt-1">
                <a href="/images/{{uniqueId}}"><img src="/uploads/temp/{{filename}}"
                        class="imgthumbnail img-fluid img-popular"></a>
            </div>

            {{/each}}
        </div>
    </div>
</div>