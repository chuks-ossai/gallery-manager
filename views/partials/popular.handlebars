<div class="card mt-3">
    <div class="card-header">
        <h5 class="panel-title">Most Popular</h5>
    </div>
    <div class="card-body">
        <div class="row">
            {{#each sidebar.popular}}
            <div class="col-md-6 p-2">
                <a href="/images/{{uniqueId}}"><img src="/public/ upload/{{filename}}"
                        class="imgthumbnail img-fluid"></a>
            </div>

            {{/each}}
        </div>
    </div>
</div>